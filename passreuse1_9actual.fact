#!/bin/sh
#1.9  Ensure IAM password policy prevents password reuse

cmd=$(aws iam get-account-password-policy | grep "PasswordReusePrevention")
if [ $(aws iam get-account-password-policy | grep "PasswordReusePrevention" | wc -l ) -eq 0 ] ; then
   echo "{ \"1.9 IAM password policy prevents password reuse\" : \"\"}"
else
   echo "{ \"1.9 PASSWORD REUSE\" : \"$cmd\"}"
fi
